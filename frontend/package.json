{
  "name": "frostgate-foundry-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "up": "docker compose -f infra/docker-compose.yml --env-file infra/.env up -d --build",
    "down": "docker compose -f infra/docker-compose.yml down --remove-orphans",
    "logs": "docker compose -f infra/docker-compose.yml logs -f",
    "ps": "docker compose -f infra/docker-compose.yml ps",
    "rebuild-frontend": "docker compose -f infra/docker-compose.yml --env-file infra/.env up -d --build frontend"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "vite": "^4.0.0",
    "@vitejs/plugin-react": "^4.0.0"
  
  }
}